{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "2.0.5",
  "name": "@pothos-examples/prisma",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "generate": "prisma generate",
    "seed": "prisma migrate reset -f",
