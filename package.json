    "@pothos/core": "workspace:*",
    "@pothos/plugin-prisma": "workspace:*",
    "@prisma/client": "^3.12.0",
    "@swc-node/register": "^1.4.2",
    "graphql": "16.3.0",
    "prisma": "^3.12.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
